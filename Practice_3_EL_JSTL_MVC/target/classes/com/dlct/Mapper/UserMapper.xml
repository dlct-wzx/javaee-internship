<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dlct.Mapper.UserMapper">
    <select id="queryAllUser" resultType="com.dlct.pojo.User">
        select * from javaee_practice.user
    </select>
    <select id="queryUserByName" resultType="com.dlct.pojo.User" parameterType="String">
        select * from javaee_practice.user where name=#{name}
    </select>

</mapper>